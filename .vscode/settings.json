{
    "editor.renderWhitespace": "all",
    "editor.insertSpaces": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "editor.comments.insertSpace": false,
    "files.associations": {
        "*.tcc": "cpp",
        "algorithm": "cpp",
        "random": "cpp",
        "type_traits": "cpp",
        "functional": "cpp",
        "array": "cpp",
        "deque": "cpp",
        "string": "cpp",
        "vector": "cpp",
        "iterator": "cpp",
        "string_view": "cpp",
        "initializer_list": "cpp",
        "unordered_map": "cpp"
    }
}
